{
    "name": "Fast Selection",
    "version": "1.5",
    "manifest_version": 2,
    "description": "An assisted tool for egova faq",
    "browser_action": {
        "default_icon": "favicon.png"
    },
    "icons": {
        "48": "favicon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://faq.egova.com.cn:7777/*"
            ],
            "js": [
                "./js/content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "images/ic-clear.png"
    ]
}